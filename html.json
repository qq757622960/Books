        "prefix": "layui-btn",
        "body": "<button class=\"layui-btn layui-btn-primary\">原始按钮</button>"
    },
    // 按钮组
    "Layui BtnGroup": {
        "prefix": "layui-btn-group",
        "body": "<div class=\"layui-btn-group\">\n\t<button class=\"layui-btn\">增加</button>\n\t<button class=\"layui-btn\">删除</button>\n\t<button class=\"layui-btn\">修改</button>\n</div>"
    },
