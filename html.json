        ],
        "description": "layui-table-render"
    },
    "Layui Table": {
        "prefix": "layui-table",
        "body": [
            "<!-- lay-even: 用于开启 隔行 背景  lay-skin: 风格  lay-size: 尺寸 -->",
            "<table class=\"layui-table\" lay-even lay-skin=\"nob\" lay-size=\"lg\">",
            "\t<colgroup>",
            "\t\t<col width=\"150\"></col>",
            "\t\t<col width=\"200\"></col>",
            "\t</colgroup>",
            "\t<thead>",
            "\t\t<tr>",
            "\t\t\t<th>姓名</th>",
            "\t\t\t<th>时间</th>",
            "\t\t\t<th>状态</th>",
            "\t\t</tr>",
            "\t</thead>",
            "\t<tbody>",
            "\t\t<tr>",
            "\t\t\t<td>孟民雪</td>",
            "\t\t\t<td>2018-9-17</td>",
            "\t\t\t<td>未婚</td>",
            "\t\t</tr>",
            "\t</tbody>",
            "</table>",
        ],
        "description": "layui-table"
    },
