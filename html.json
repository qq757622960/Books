    "Layui Row": {
        "prefix": "layui-row",
        "body": "<div class=\"layui-row\">\n\t<div class=\"layui-col-md6\">\n\t</div>\n\t<div class=\"layui-col-md6\">\n\t</div>\n</div>"
    },
    "Layui Checkbox": {
        "prefix": "layui-checkbox",
        "body": "<input type=\"checkbox\" name=\"\" title=\"${1}\" lay-skin=\"primary\" checked>"
    },
    "Layui Input": {
        "prefix": "layui-input",
        "body": "<input type=\"text\" name=\"${1}\" required lay-verify=\"required\" placeholder=\"${2}\" autocomplete=\"off\" class=\"layui-input\">"
